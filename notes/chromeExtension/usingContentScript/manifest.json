{
    "manifest_version": 2,
    "version": "1",
    "name": "Contamination",
    "description": "Learning to use content scripts.",
    "browser_action": {
        "default_title": "Contamination"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions":["activeTab"]
}